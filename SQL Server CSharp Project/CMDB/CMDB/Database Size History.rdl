  <DataSets>
    <DataSet Name="DatabaseSize">
      <Query>
        <DataSourceName>CMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@instance_id">
            <Value>=Parameters!instance_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DatabaseName">
            <Value>=Parameters!DatabaseName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CAST(ds.collection_date AS DATE) as collected_date, d.db_name as database_name, MAX(size) as size
FROM dbo.tbl_db_size_log  ds
INNER JOIN dbo.tbl_db d on d.db_id = ds.db_id
WHERE instance_id = @instance_id
and d.db_id IN (@DatabaseName)
and ds.collection_date BETWEEN @DateFrom AND @DateTo
GROUP BY CAST(ds.collection_date AS DATE), d.db_name
ORDER BY collected_date asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="collected_date">
          <DataField>collected_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="database_name">
          <DataField>database_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="size">
          <DataField>size</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Instance">
      <Query>
        <DataSourceName>CMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@internal_instance_id">
            <Value>=Parameters!internal_instance_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InstanceNameSearch">
            <Value>=Parameters!InstanceNameSearch.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @internal_instance_id IS NULL 
SELECT instance_id, instance_name FROM dbo.tbl_sql_instance WHERE instance_name LIKE '%' + @InstanceNameSearch + '%' ORDER BY instance_name 
ELSE
SELECT instance_id, instance_name FROM dbo.tbl_sql_instance WHERE instance_id = @internal_instance_id</CommandText>
      </Query>
      <Fields>
        <Field Name="instance_id">
          <DataField>instance_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="instance_name">
          <DataField>instance_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Database">
      <Query>
        <DataSourceName>CMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@instance_id">
            <Value>=Parameters!instance_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT db_id, db_name
FROM dbo.tbl_db
WHERE instance_id = @instance_id
ORDER BY CASE WHEN CAST(db_identifier as INT) &lt;=4 THEN 1 ELSE 0 END, db_name</CommandText>
      </Query>
      <Fields>
        <Field Name="db_id">
          <DataField>db_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="db_name">
          <DataField>db_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Instance_Default">
      <Query>
        <DataSourceName>CMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@internal_instance_id">
            <Value>=Parameters!internal_instance_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @internal_instance_id IS NULL 
SELECT NULL as id
ELSE
SELECT id FROM dbo.tbl_instance WHERE id = @internal_instance_id</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
