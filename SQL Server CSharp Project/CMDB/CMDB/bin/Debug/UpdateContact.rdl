    <DataSet Name="Update">
      <Query>
        <DataSourceName>CMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@contact">
            <Value>=Parameters!Contact.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@description">
            <Value>=Parameters!Description.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@instance_id">
            <Value>=Parameters!instance_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Update">
            <Value>=Parameters!Update.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET NOCOUNT ON

UPDATE dbo.tbl_instance
SET  
contact = ISNULL(@contact, contact),
description = ISNULL(@description, description)
where id = @instance_id AND (@Update = 1)

SELECT contact, description FROM dbo.tbl_instance where id = @instance_id</CommandText>
      </Query>
